{
  "expo": {
    "scheme": "tritiumsabers",
    "plugins": ["./ble-manager-plugin"],
    "name": "Tritium Sabers",
    "slug": "tritiumsabers",
    "version": "0.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "owner": "tritiumstudios",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/images/icon.png",
    "android": {
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_ADVERTISE"
      ],
      "package": "com.tritiumstudios.tritiumsabers",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "bb06b34c-fcaa-4adc-857f-30e63c231137"
      }
    },
    "ios": {
      "bundleIdentifier": "com.tritiumstudios.tritiumsabers",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to use Bluetooth"
      }
    }
  }
}

